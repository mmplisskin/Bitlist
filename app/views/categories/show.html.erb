
<div class="container">




<h1><%=@category.name%></h1>
<%=link_to "All categories", root_path, class:'h4'%>






      <%@category.items.each do |item|%>

      <div class="row">
          <center>
            <h2><%=link_to item.name, category_item_path(@category.name, item.id)%></h2>
              <%= item.city %>
              <%= item.zipcode %>
          </center>

      </div>
  <%end%>

</div>
